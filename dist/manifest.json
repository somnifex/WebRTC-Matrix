{
  "manifest_version": 3,
  "name": "WebRTC Matrix",
  "version": "1.0.0",
  "description": "Cross-browser WebRTC permission manager with per-domain control and WebDAV sync.",
  "permissions": [
    "storage",
    "privacy",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader-BSsq8ort.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "options_page": "src/options/index.html",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/storage-BF9_fGL3.js",
        "assets/match-C4wd3_Kx.js",
        "assets/index.ts-NUyobQFu.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
